{
    // "env": {
    //     "browser": true,
    //     "es2020": true
    // },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    // "parserOptions": {
    //     "ecmaFeatures": {
    //         "jsx": true
    //     },
    //     "ecmaVersion": 11,
    //     "sourceType": "module"
    // },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/react-in-jsx-scope": "off",
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "linebreak-style": ["error", "windows"],
        "no-console": "off",
        "quotes": ["error", "tab"],
        "indent": ["error", 4]
    },
    "globals": {
        "React": "writable"
      }
}
